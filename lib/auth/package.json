{
  "name": "@nrwlz/auth",
  "version": "0.0.1",
  "private": true,
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "development": "./src/index.ts",
      "types": "./dist/src/index.d.ts",
      "import": "./dist/src/index.js",
      "default": "./dist/src/index.js"
    },
    "./types": {
      "development": "./src/types.ts",
      "types": "./dist/src/types.d.ts"
    },
    "./plugins": {
      "development": "./src/plugins/index.ts",
      "types": "./dist/src/plugins/client/index.d.ts",
      "import": "./dist/src/plugins/client/index.js",
      "default": "./dist/src/plugins/index.js"
    },
    "./plugins/client": {
      "development": "./src/plugins/client/index.ts",
      "types": "./dist/src/plugins/client/index.d.ts",
      "import": "./dist/src/plugins/client/index.js",
      "default": "./dist/src/plugins/client/index.js"
    }
  },
  "dependencies": {
    "tslib": "^2.3.0",
    "@nrwlz/shared": "workspace:*"
  },
  "nx": {
    "targets": {
      "build": {
        "executor": "@nx/js:tsc",
        "outputs": [
          "{options.outputPath}"
        ],
        "options": {
          "outputPath": "lib/auth/dist",
          "tsConfig": "lib/auth/tsconfig.lib.json",
          "packageJson": "lib/auth/package.json",
          "main": "lib/auth/src/index.ts",
          "assets": [
            "lib/auth/*.md"
          ]
        }
      }
    }
  }
}
