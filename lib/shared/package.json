{
  "name": "@nrwlz/shared",
  "version": "0.0.1",
  "private": true,
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "development": "./src/index.ts",
      "types": "./dist/src/index.d.ts",
      "import": "./dist/src/index.ts",
      "default": "./dist/src/index.js"
    },
    "./types/*": {
      "development": "./src/types/*.ts",
      "types": "./dist/src/types/*.d.ts",
      "import": "./dist/src/types/*.js",
      "default": "./dist/src/types/*.js"
    }
  },
  "dependencies": {
    "tslib": "^2.3.0"
  },
  "nx": {
    "targets": {
      "build": {
        "executor": "@nx/js:tsc",
        "outputs": [
          "{options.outputPath}"
        ],
        "options": {
          "outputPath": "lib/shared/dist",
          "tsConfig": "lib/shared/tsconfig.lib.json",
          "packageJson": "lib/shared/package.json",
          "main": "lib/shared/src/index.ts",
          "assets": [
            "lib/shared/*.md"
          ]
        }
      }
    }
  }
}
